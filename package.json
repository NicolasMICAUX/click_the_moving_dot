{
  "name": "click_the_moving_dot_code",
  "version": "1.0.0",
  "description": "Real-time HTML5 game where users click a moving dot that tries to escape. Built for Google Cloud with efficient data collection.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:local": "DISABLE_CLOUD=true node server.js",
    "start:onnx": "node server.js --onnx",
    "start:local-onnx": "DISABLE_CLOUD=true node server.js --onnx",
    "dev": "nodemon server.js",
    "dev:local": "DISABLE_CLOUD=true nodemon server.js",
    "build": "node build.js",
    "build:deploy": "npm run build && gcloud app deploy --quiet",
    "deploy": "npm run build:deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "game",
    "html5",
    "canvas",
    "websocket",
    "realtime",
    "google-cloud",
    "mouse-tracking",
    "ai-ready"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/firestore": "^7.11.3",
    "@google-cloud/storage": "^7.12.1",
    "apache-arrow": "^21.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "parquetjs-lite": "^0.8.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "clean-css": "^5.3.3",
    "fs-extra": "^11.3.1",
    "html-minifier-terser": "^7.2.0",
    "nodemon": "^3.1.10",
    "terser": "^5.43.1"
  }
}
